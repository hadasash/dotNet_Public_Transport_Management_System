<Window x:Class="PL.AddStationInLine"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="AddStationInLine" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="cbStations" HorizontalAlignment="Left" Margin="356,95,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="badd" Content="Add" HorizontalAlignment="Left" Margin="356,183,0,0" VerticalAlignment="Top" Width="120" Click="badd_Click"/>

    </Grid>
</Window>
